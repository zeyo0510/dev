{
   "app": {
      "background": {
         "scripts": [ "simpl.js", "loader.js", "modules/crypto.js", "modules/database.js", "modules/html.js", "modules/http.js", "modules/socket.js", "modules/string.js", "modules/system.js", "modules/websocket.js", "background.js" ]
      }
   },
   "description": "Simpl.js lets you write and run full-stack javascript apps in your browser",
   "externally_connectable": {
      "matches": [ "https://simpljs.com/*" ]
   },
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "33",
   "name": "Simpl.js",
   "offline_enabled": true,
   "permissions": [ "unlimitedStorage", "system.cpu", "system.display", "system.memory", "system.network", "system.storage", "browser", "http://*/", "https://*/", "ftp://*/" ],
   "short_name": "Simpl.js",
   "sockets": {
      "tcp": {
         "connect": "*"
      },
      "tcpServer": {
         "listen": "*"
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "url_handlers": {
      "launch": {
         "matches": [ "https://simpljs.com/launch*" ],
         "title": "Launch Simpl.js Console"
      }
   },
   "version": "0.76"
}
