{
  "manifest_version": 2,
  "name": "``projectName``",
  "short_name": "``projectName``",
  "description": "",
  "version": "0.0.1",
  "minimum_chrome_version": "38",

  "icons": {
    "16": "assets/icon_16.png",
    "128": "assets/icon_128.png"
  },
  "background": {
