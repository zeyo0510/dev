{
"update_url": "https://clients2.google.com/service/update2/crx",

    "default_locale": "en",
    "name": "__MSG_application_title__",
    "version": "5.1.1",
    "manifest_version": 2,
    "description": "__MSG_application_description__",
    "oauth2": {
        "client_id": "48502896198-ug5r554dp6nao483m47b4tr5uvfv9fj1.apps.googleusercontent.com",
        "scopes": [
          "https://www.googleapis.com/auth/drive.readonly"
        ]
    },
    "permissions": [
      {
        "mediaGalleries": ["read", "allAutoDetected"]
      },
      "storage", "<all_urls>", "notifications", "unlimitedStorage", "identity", "alwaysOnTopWindows",
      { "fileSystem": ["write", "retainEntries", "directory"] }
    ],
    "icons": {
        "128": "icon_128.png",
        "48": "icon_48.png",
        "16": "icon_16.png"
    },
    "sockets": {
      "tcp": {
        "connect": "*"
      },
      "tcpServer": {
        "listen": "*"
      }
    },
    "offline_enabled": true,
    "app": {
        "background": {
          "scripts": ["js/bg.js"]
        }
    },
    "commands": {
      "cmd-prev": {
        "suggested_key": {
          "default": "MediaPrevTrack"
        },
        "description": "__MSG_app_cmd_prev__",
        "global": true
      },
      "cmd-next": {
        "suggested_key": {
          "default": "MediaNextTrack"
        },
        "description": "__MSG_app_cmd_next__",
        "global": true
      },
      "cmd-PlayPause": {
        "suggested_key": {
          "default": "MediaPlayPause"
        },
        "description": "__MSG_app_cmd_playpause__",
        "global": true
      }
    }
}
