{
   "app": {
      "background": {
         "scripts": [ "fontBase64.js", "socketClient.js", "background.js" ]
      }
   },
   "default_locale": "en_US",
   "description": "PttChrome provides telnet connections to ptt.cc.",
   "externally_connectable": {
      "matches": [ "*://iamchucky.github.io/*" ]
   },
   "icons": {
      "128": "icon_128.png",
      "16": "icon_16.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "42",
   "name": "PttChrome",
   "permissions": [ "webview", "clipboardRead", "clipboardWrite", "storage", "browser" ],
   "sockets": {
      "tcp": {
         "connect": "*:*"
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.3"
}
