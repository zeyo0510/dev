{
   "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },
   "author": {
      "name": "https://crowdemu.com/"
   },
   "description": "Play your favorite NES, SNES, Gameboy and Gameboy Advance ROMs; use your phone as your controller - or use custom keyboard mappings.",
   "file_handlers": {
      "gpemu": {
         "extensions": [ "32x", "gb", "gba", "gbc", "gen", "md", "mgw", "nes", "sfc", "smc", "smd", "sms", "vec" ]
      }
   },
   "icons": {
      "128": "/frontEnd/img/icon/snes_128.png",
      "16": "/frontEnd/img/icon/snes_16.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "RetroSuite EMU",
   "offline_enabled": true,
   "permissions": [ "power", "system.network", {
      "socket": [ "tcp-listen" ]
   }, {
      "fileSystem": [ "retainEntries", "directory", "write" ]
   }, "pointerLock", "fullscreen", "identity", "storage", "unlimitedStorage" ],
   "short_name": "RetroSuite",
   "sockets": {
      "tcp": {
         "connect": "*"
      },
      "tcpServer": {
         "listen": "*"
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.6"
}
